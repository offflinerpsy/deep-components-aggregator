{
  "$schema":"https://json-schema.org/draft/2020-12/schema",
  "type":"object",
  "properties":{
    "results":{
      "type":"array",
      "items":{
        "type":"object",
        "required":["mpn","title","manufacturer","regions","price_min_rub"],
        "properties":{
          "mpn":{"type":"string","minLength":1},
          "title":{"type":"string"},
          "description":{"type":"string"},
          "manufacturer":{"type":"string"},
          "package":{"type":"string"},
          "packaging":{"type":"string","enum":["Tube","Reel","Tape","Cut Tape","Tray"]},
          "regions":{"type":"array","items":{"enum":["EU","US","ASIA"]}},
          "stock_total":{"type":"integer","minimum":0},
          "price_min":{"type":"number","minimum":0},
          "price_min_currency":{"type":"string","enum":["USD","EUR"]},
          "price_min_rub":{"type":"number","exclusiveMinimum":0},
          "image":{"type":"string","format":"uri"}
        }
      }
    }
  },
  "required":["results"],
  "additionalProperties":false
}
