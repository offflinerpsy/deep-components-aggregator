{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SearchRow",
  "type": "object",
  "required": ["mpn", "title", "manufacturer", "description_short", "package", "packaging", "regions", "product_url", "source"],
  "properties": {
    "mpn": {
      "type": "string",
      "minLength": 1,
      "maxLength": 64
    },
    "title": {
      "type": "string",
      "minLength": 1,
      "maxLength": 256
    },
    "manufacturer": {
      "type": "string",
      "maxLength": 128
    },
    "description_short": {
      "type": "string",
      "maxLength": 200
    },
    "package": {
      "type": "string",
      "maxLength": 32
    },
    "packaging": {
      "type": "string",
      "maxLength": 32
    },
    "regions": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": ["RU", "EU", "US", "ASIA"]
      },
      "minItems": 1
    },
    "stock": {
      "type": ["number", "null"],
      "minimum": 0
    },
    "min_price_rub": {
      "type": ["number", "null"],
      "minimum": 0
    },
    "min_currency": {
      "type": ["string", "null"],
      "enum": ["RUB", "USD", "EUR", null]
    },
    "image_url": {
      "type": ["string", "null"],
      "format": "uri"
    },
    "product_url": {
      "type": "string",
      "format": "uri"
    },
    "source": {
      "type": "string",
      "enum": ["chipdip", "promelec", "compel", "electronshik", "elitan", "oemstrade"]
    }
  },
  "additionalProperties": false
}
